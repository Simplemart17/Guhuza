generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  user_id    Int     @id @default(autoincrement())
  user_email String? @unique(map: "user_email_UNIQUE") @db.VarChar(45)
  address    String? @db.VarChar(45)
}
